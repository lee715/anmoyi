<button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" name=<%= id %>>
  <span class="btn-val"><%= name %></span> <span class="caret"></span>
</button>
<ul class="dropdown-menu">
  <li><a href="javascript:;" class="selector" data-id=<%= id %>>all</a></li>
  <% for(var i=0,len=data.length;i<len;i++){ %>
  <li><a href="javascript:;" class="selector" data-id=<%= id %>><%= data[i] %></a></li>
  <% } %>
</ul>
