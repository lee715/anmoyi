<form class="form-horizontal" >
  <div class="form-group">
    <label for="name" class="col-sm-2 control-label">类型</label>
    <div class="col-sm-10">
      <%if(types) {%>
      <select class="form-control" name="name">
          <% for(var i=0,len=types.length;i<len;i++){ %>
          <option value=<%=types[i].name%>><%=types[i].name%></option>
          <% } %>
      </select>
      <%} else {%>
      <input type="text" name="name" class="form-control" />
      <%}%>
    </div>
  </div>
  <div class="form-group">
    <label for="uid" class="col-sm-2 control-label">价格</label>
    <div class="col-sm-10">
      <input type="text" name="price" class="form-control" placeholder="多个价格以逗号分隔；如8,10" />
    </div>
  </div>
  <div class="form-group">
    <label for="time" class="col-sm-2 control-label">服务时间</label>
    <div class="col-sm-10">
      <input type="text" name="time" class="form-control" placeholder="多个时间以逗号分隔；如8,10分钟" />
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default">提交</button>
      <%if(types) {%>
      <a href="javascript:;" class="btn btn-default del">删除</a>
      <%}%>
    </div>
  </div>
</form>